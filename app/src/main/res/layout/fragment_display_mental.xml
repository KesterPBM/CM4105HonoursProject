<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvMentalTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:background="#00FFFFFF"
        android:text="Symptoms"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/tvMentalDescription"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvMentalSymptoms"
        android:layout_width="0dp"
        android:layout_height="267dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:background="#00FFFFFF"
        android:text="@string/stockdataplacehold"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvMentalSymptoms2" />

    <TextView
        android:id="@+id/tvMentalSymptoms2"
        android:layout_width="319dp"
        android:layout_height="45dp"
        android:background="#00FFFFFF"
        android:text="Symptoms"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/tvMentalDescription"
        app:layout_constraintTop_toBottomOf="@+id/tvMentalDescription" />

    <TextView
        android:id="@+id/tvMentalDescription"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="402dp"
        android:background="#00FFFFFF"
        android:text="@string/stockdataplacehold"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/btMentalTreatment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvMentalTitle" />

    <Button
        android:id="@+id/btMentalTreatment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginBottom="16dp"
        android:text="@string/btredownload"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvMentalDescription" />
</androidx.constraintlayout.widget.ConstraintLayout>